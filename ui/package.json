{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.1",
    "@material-ui/data-grid": "^4.0.0-alpha.33",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "fuse.js": "^6.4.6",
    "graphql": "^15.5.1",
    "graphql-request": "^3.4.0",
    "json-stringify-pretty-compact": "^3.0.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-highlight.js": "^1.0.7",
    "react-query": "^3.18.1",
    "react-router-dom": "^5.2.0",
    "recoil": "^0.3.1",
    "styled-components": "^5.3.0"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "tsc --noEmit && eslint --ext .tsx,.ts,.js src",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "graphql:codegen:watch": "nodemon --exec \"yarn graphql:codegen\" --ext .graphql --delay 5",
    "start": ". env/.env.local && react-scripts start",
    "start:codegen": "concurrently \"yarn start\" \"yarn graphql:codegen:watch\"",
    "build": "react-scripts build",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.7",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.16",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "@graphql-codegen/typescript-react-query": "^1.3.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-highlight": "^0.12.3",
    "@types/react-highlight.js": "^1.0.1",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.11",
    "concurrently": "^6.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "node-sass": "5.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "rc": "^1.2.8",
    "react-scripts": "4.0.3",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.1.2"
  }
}
